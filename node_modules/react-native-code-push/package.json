      "sharedLibraries": [
        "libz"
      ]
    },
    "params": [
      {
        "type": "input",
        "name": "androidDeploymentKey",
        "message": "What is your CodePush deployment key for Android (hit <ENTER> to ignore)"
      }
    ],
    "commands": {
      "postlink": "node node_modules/react-native-code-push/scripts/postlink/run"
    }
  },
